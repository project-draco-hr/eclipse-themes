{
  String bundleId=addon.getContributorURI().substring("plugin://".length());
  String uri=addon.getContributionURI();
  Bundle bundle=Platform.getBundle(bundleId);
  if (bundle == null) {
    return false;
  }
  IPath path=new Path(uri);
  Class<?> type=null;
  try {
    type=bundle.loadClass(path.lastSegment());
  }
 catch (  ClassNotFoundException e) {
    return false;
  }
  return type != null;
}
