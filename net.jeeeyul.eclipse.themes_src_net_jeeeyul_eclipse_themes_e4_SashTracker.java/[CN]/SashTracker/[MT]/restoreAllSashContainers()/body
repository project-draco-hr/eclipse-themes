{
  lock.acquire();
  Composite[] array=sashContainers.toArray(new Composite[sashContainers.size()]);
  lock.release();
  for (  Composite each : array) {
    try {
      updateSash(each,4);
    }
 catch (    Exception e) {
      e.printStackTrace();
      ChromeThemeCore.getDefault().log(e);
    }
  }
}
