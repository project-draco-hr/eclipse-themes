{
  Display display=Display.getDefault();
  Shell shell=new Shell(display);
  shell.setLayout(new FillLayout());
  HueCanvas canvas=new HueCanvas(shell,SWT.NORMAL);
  canvas.setHue(160f);
  shell.pack();
  shell.open();
  while (!shell.isDisposed()) {
    if (!display.readAndDispatch()) {
      display.sleep();
    }
  }
}
