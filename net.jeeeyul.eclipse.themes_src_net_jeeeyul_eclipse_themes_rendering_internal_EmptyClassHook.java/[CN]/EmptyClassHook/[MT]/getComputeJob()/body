{
  if (computeJob == null) {
    computeJob=new UIJob(Display.getDefault(),"Update Empty Class"){
      public IStatus runInUIThread(      IProgressMonitor monitor){
        if (!isDisposed) {
          computeEmptyClass();
        }
        return Status.OK_STATUS;
      }
    }
;
    computeJob.setUser(false);
    computeJob.setSystem(true);
  }
  return computeJob;
}
