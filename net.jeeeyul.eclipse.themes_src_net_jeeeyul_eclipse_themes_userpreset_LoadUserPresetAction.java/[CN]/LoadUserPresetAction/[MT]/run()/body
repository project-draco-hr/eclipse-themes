{
  try {
    PreferenceStore store=new PreferenceStore();
    new ChromePreferenceInitializer().initializeDefaultPreferences(store);
    byte[] data=userPreset.getContents().getBytes("8859_1");
    InputStream stream=new ByteArrayInputStream(data);
    store.load(stream);
    stream.close();
    applyFallBackFont(store);
    ArrayList<ChromePage> pages=getPrefererncePage().getPages();
    for (    ChromePage each : pages) {
      each.load(store);
    }
  }
 catch (  IOException e) {
    ChromeThemeCore.getDefault().getLog().log(new Status(IStatus.ERROR,ChromeThemeCore.PLUGIN_ID,"Applying preset results some errors, default settings was applied.",e));
    fallBackToDefaults();
  }
  super.run();
}
